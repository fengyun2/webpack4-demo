{
  "name": "webpack4-demo",
  "version": "1.0.2",
  "description": "webpack4-demo",
  "main": "index.js",
  "scripts": {
    "start": "npm run dll && npm run dev",
    "prod": "npm run dll && npm run build",
    "dll": "npm run clean-dll && webpack --config config/webpack.dll.config.js --mode production",
    "dev": "cross-env NODE_ENV='development' webpack-dev-server --open --config config/webpack.config.dev.js --mode development",
    "build": "npm run clean-dist && cross-env NODE_ENV='production' webpack --config config/webpack.config.prod.js --display-used-exports --optimize-minimize --mode production",
    "clean-dist": "rimraf dist",
    "clean-dll": "rimraf dll",
    "commit": "git-cz",
    "prettier": "prettier --write \"{src,mock}/**/*.{js,css,scss,less}\"",
    "release": "standard-version",
    "eslint": "eslint --fix src/**/*.js",
    "post-update": "yarn upgrade --latest"
  },
  "keywords": [
    "webpack",
    "webpack4",
    "webpack5"
  ],
  "author": "fengyun2",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "autodll-webpack-plugin": "^0.4.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-import": "^1.11.0",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^1.0.1",
    "commitizen": "^3.0.5",
    "commitlint-config-cz": "^0.11.1",
    "copy-webpack-plugin": "^4.4.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "cz-customizable": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "glob-all": "^3.1.0",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "husky": "^1.3.1",
    "less": "^3.7.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.14",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "standard-version": "^4.3.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.6.2",
    "vue-template-compiler": "^2.6.6",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3",
    "webpack-parallel-uglify-plugin": "^1.1.0",
    "webpackbar": "^3.1.5"
  },
  "dependencies": {
    "antd": "^3.13.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-loadable": "^5.5.0",
    "vue": "^2.6.6"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  }
}
